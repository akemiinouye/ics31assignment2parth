#include "grades.h"
#include <iostream>
using namespace std;

Grades::Grades(){}

double Grades::divideScores(double rawScore, double totalPossible){
    return rawScore/totalPossible;
}

double Grades::calcWeightedScore(double score, double weight){
    return score*weight;
}

void Grades::createArray(){
    cin >> columnSize;
    a = new double[columnSize];
    for (int i = 0; i < columnSize; i++) {
        cin >> a[i]; 
    }
    }






